c={1:['swetha','manager',20000,'kakinada'],2:['supraja','manager',15000,'kakinada'],3:['surya','manager',18000,'kakinada'],4:['sai','manager',10000,'kakinada'],5:['ramya','manager',5000,'kakinada']}
x = int(input('the emp no:'))
print('name of the emp:',c[x][0])
print('emp design:',c[x][1])
print('emp sal:',c[x][2])
print('emp city:',c[x][3])
